  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "test": "echo \"Skipping tests\" && exit 0",
    "prebuild": "rm -rf ./public",
    "build": "gatsby build",
    "deploy:prod": "S3PATH=latest ./scripts/deploy.sh",
    "deploy:dev": "S3PATH=dev ./scripts/deploy.sh",
    "deploy": "./scripts/deploy.sh",
    "s3:backup": "aws s3 sync s3://site-eddywashere.com/latest s3://site-eddywashere.com/revert --delete",
    "s3:upload": "aws s3 sync ./public s3://site-eddywashere.com/$S3PATH --acl public-read --sse --delete",
    "s3:revert": "aws s3 sync s3://site-eddywashere.com/revert s3://site-eddywashere.com/latest --acl public-read --sse --delete"
  }
}
